		-term "__terminate_Kerberos5GlobalsLib" \
		-map Krb5GlobalsLib.PPC.debug.MAP -o Kerberos5GlobalsLib.PPC.debug \
		{krb5-globals-obj-ppc} {standard-libraries-ppc-debug} {ccache-ppc-debug} {krb5-globals-data-ppc}

