AUTH_DEF=-DAUTHENTICATION -DENCRYPTION -DDES_ENCRYPTION -DKRB5 -DFORWARD \
	-UNO_LOGIN_F -DLOGIN_CAP_F -DLOGIN_PROGRAM=KRB5_PATH_LOGIN
LOCALINCLUDES=-I.. -I$(srcdir)/.. @KRB4_INCLUDES@
CFLAGS = $(CCOPTS) $(AUTH_DEF) $(DEFS) $(LOCALINCLUDES)
LIBOBJS=@LIBOBJS@

LIB=    libtelnet.a
SRCS=   $(srcdir)/auth.c \
	$(srcdir)/encrypt.c \
	$(srcdir)/genget.c \
	$(srcdir)/misc.c \
	$(srcdir)/kerberos.c \
	$(srcdir)/kerberos5.c \
	$(srcdir)/forward.c \
	$(srcdir)/spx.c \
	$(srcdir)/enc_des.c \
	$(srcdir)/setenv.c \
	$(srcdir)/getent.c \
	$(scrdir)/parsetos.c \
	$(srcdir)/strdup.c \
	$(srcdir)/strcasecmp.c \
	$(srcdir)/strchr.c \
	$(srcdir)/strrchr.c \
	$(srcdir)/strftime.c \
	$(srcdir)/strerror.c

OBJS=   auth.o encrypt.o genget.o \
	misc.o kerberos.o kerberos5.o forward.o spx.o enc_des.o \
	$(LIBOBJS)

TELNET_H= $(srcdir)/../arpa/telnet.h

all:: $(LIB)
$(LIB): $(OBJS)
	$(RM) $(LIB)
	$(ARADD) $@ $(OBJS)
	$(RANLIB) $@

clean::
	$(RM) $(LIB)

auth.o: $(TELNET_H)
auth.o: encrypt.h
auth.o: auth.h
auth.o: misc-proto.h
encrypt.o: $(TELNET_H)
encrypt.o: encrypt.h
encrypt.o: misc.h
kerberos.o: $(TELNET_H)
kerberos.o: encrypt.h
kerberos.o: auth.h
kerberos.o: misc.h
kerberos5.o: $(TELNET_H)
kerberos5.o: encrypt.h
kerberos5.o: auth.h
kerberos5.o: misc.h
misc.o: misc.h
enc_des.o: $(TELNET_H)
enc_des.o: encrypt.h
enc_des.o: key-proto.h
enc_des.o: misc-proto.h
install::
