all-unix:: all-liblinks
install-unix:: install-libs

libcrypto.lib:
	libdir crypto.lib

clean-unix:: clean-liblinks clean-libs clean-libobjs

check-unix:: t_nfold
	$(RUN_SETUP) ./t_nfold

t_nfold$(EXEEXT): t_nfold.$(OBJEXT) nfold.$(OBJEXT)
	$(CC_LINK) -o $@ t_nfold.$(OBJEXT) nfold.$(OBJEXT)

clean::
	$(RM) t_nfold.o t_nfold

