thisconfigdir=./..
myfulldir=lib/crypto/dk
mydir=dk
BUILDTOP=$(REL)$(U)$(S)$(U)$(S)$(U)
CFLAGS = $(CCOPTS) $(DEFS) -I$(srcdir)/..

##DOS##BUILDTOP = ..\..\..
##DOS##PREFIXDIR=dk
##DOS##OBJFILE=..\$(OUTPRE)dk.lst
##WIN16##LIBNAME=..\crypto.lib

PROG_LIBPATH=-L$(TOPLIBD)
PROG_RPATH=$(KRB5_LIBDIR)

RUN_SETUP = @KRB5_RUN_ENV@ KRB5_CONFIG=$(SRCTOP)/config-files/krb5.conf

STLIBOBJS=\
	checksum.o	\
	dk_decrypt.o	\
	dk_encrypt.o	\
	derive.o	\
	stringtokey.o

OBJS=\
	$(OUTPRE)checksum.$(OBJEXT)	\
	$(OUTPRE)dk_decrypt.$(OBJEXT)	\
	$(OUTPRE)dk_encrypt.$(OBJEXT)	\
	$(OUTPRE)derive.$(OBJEXT)	\
	$(OUTPRE)stringtokey.$(OBJEXT)

SRCS=\
	$(srcdir)/checksum.c	\
	$(srcdir)/dk_decrypt.c	\
	$(srcdir)/dk_encrypt.c	\
	$(srcdir)/derive.c	\
	$(srcdir)/stringtokey.c

##DOS##LIBOBJS = $(OBJS)

all-unix:: all-libobjs

includes:: depend

depend:: $(SRCS)

clean-unix:: clean-libobjs
