mydir=lib/crypto/builtin/enc_provider
BUILDTOP=$(REL)..$(S)..$(S)..$(S)..
LOCALINCLUDES = -I$(srcdir)/../des 	\
		-I$(srcdir)/../arcfour \
		-I$(srcdir)/../aes 	\
		-I$(srcdir)/../../krb 			\
		-I$(srcdir)/../../krb/rand2key	 	\
		-I$(srcdir)/.. -I$(srcdir)/. 
DEFS=

##DOS##BUILDTOP = ..\..\..\..
##DOS##PREFIXDIR=enc_provider
