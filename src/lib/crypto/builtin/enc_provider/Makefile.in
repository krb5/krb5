STLIBOBJS= \
	des.o 	\
	des3.o 	\
	rc4.o 	\
	aes.o 

OBJS= \
	$(OUTPRE)des.$(OBJEXT) 	\
	$(OUTPRE)des3.$(OBJEXT) 	\
	$(OUTPRE)aes.$(OBJEXT) 	\
	$(OUTPRE)rc4.$(OBJEXT)

SRCS= \
	$(srcdir)/des.c 	\
	$(srcdir)/des3.c 	\
	$(srcdir)/aes.c 	\
	$(srcdir)/rc4.c

##DOS##LIBOBJS = $(OBJS)

all-unix:: all-libobjs

includes:: depend

