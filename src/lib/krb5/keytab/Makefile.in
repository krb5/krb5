thisconfigdir=./..
myfulldir=lib/krb5/keytab
mydir=keytab
BUILDTOP=$(REL)$(U)$(S)$(U)$(S)$(U)
LOCAL_SUBDIRS = file

CFLAGS = $(CCOPTS) $(DEFS)

##DOS##BUILDTOP = ..\..\..
##DOS##PREFIXDIR=keytab
##DOS##OBJFILE=..\$(OUTPRE)$(PREFIXDIR).lst
##WIN16##LIBNAME=..\krb5.lib

MAC_SUBDIRS = file

STLIBOBJS= \
	ktadd.o		\
	ktbase.o	\
	ktdefault.o	\
	ktfr_entry.o	\
	ktremove.o	\
	read_servi.o

OBJS=	\
	$(OUTPRE)ktadd.$(OBJEXT)		\
	$(OUTPRE)ktbase.$(OBJEXT)	\
	$(OUTPRE)ktdefault.$(OBJEXT)	\
	$(OUTPRE)ktfr_entry.$(OBJEXT)	\
	$(OUTPRE)ktremove.$(OBJEXT)	\
	$(OUTPRE)read_servi.$(OBJEXT)

SRCS=	\
	$(srcdir)/ktadd.c	\
	$(srcdir)/ktbase.c	\
	$(srcdir)/ktdefault.c	\
	$(srcdir)/ktfr_entry.c	\
	$(srcdir)/ktremove.c	\
	$(srcdir)/read_servi.c

all-windows:: subdirs $(OBJFILE)

##DOSsubdirs:: file\$(OUTPRE)file.lst
##DOSfile\$(OUTPRE)file.lst::
##DOS	cd file
##DOS	@echo Making in krb5\keytab\file
##DOS	-$(MAKE) -$(MFLAGS)
##DOS	cd ..

##DOS$(OBJFILE): $(OBJS) file\$(OUTPRE)file.lst
##DOS	$(RM) $(OBJFILE)
##WIN16##	$(CP) nul: $(OBJFILE)
##WIN32##	$(LIBECHO) -p $(PREFIXDIR)\ $(OUTPRE)*.obj file\$(OUTPRE)*.obj > $(OBJFILE)

all-unix:: all-libobjs
clean-unix:: clean-libobjs

clean-windows::
	cd file
	@echo Making clean in krb5\keytab\file
	-$(MAKE) -$(MFLAGS) clean
	cd ..
	@echo Making clean in krb5\keytab
	$(RM) $(OBJFILE)

