thisconfigdir=./..
myfulldir=lib/krb5/rcache
mydir=rcache
BUILDTOP=$(REL)$(U)$(S)$(U)$(S)$(U)
CFLAGS = $(CCOPTS) $(DEFS)

##DOS##BUILDTOP = ..\..\..
##DOS##PREFIXDIR=rcache
##DOS##OBJFILE=..\$(OUTPRE)$(PREFIXDIR).lst
##WIN16##LIBNAME=..\krb5.lib

STLIBOBJS = \
	rc_base.o	\
	rc_dfl.o 	\
	rc_io.o		\
	rcdef.o		\
	rc_conv.o	\
	ser_rc.o

OBJS=	\
	$(OUTPRE)rc_base.$(OBJEXT)	\
	$(OUTPRE)rc_dfl.$(OBJEXT) 	\
	$(OUTPRE)rc_io.$(OBJEXT)		\
	$(OUTPRE)rcdef.$(OBJEXT)		\
	$(OUTPRE)rc_conv.$(OBJEXT)	\
	$(OUTPRE)ser_rc.$(OBJEXT)

SRCS=	\
	$(srcdir)/rc_base.c	\
	$(srcdir)/rc_dfl.c 	\
	$(srcdir)/rc_io.c	\
	$(srcdir)/rcdef.c	\
	$(srcdir)/rc_conv.c	\
	$(srcdir)/ser_rc.c

##DOS##LIBOBJS = $(OBJS)

all-unix:: all-libobjs
clean-unix:: clean-libobjs
