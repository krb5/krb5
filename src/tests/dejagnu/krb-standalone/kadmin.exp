	eof {
	    fail "kadmin show $pname"
	    catch "expect_after"
	    return 0
	}
    }
    expect "Enter password for krbtest/admin@KRBTEST.COM:"
    send "adminpass$KEY\r"
    expect -re "\r.*-*Principal: $pname.*Flags: .*Last modified by: .*-*"
