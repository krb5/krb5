#++
# kadmin_show	- Test show principal function of kadmin.
# 
# Retrieves entry for $pname.  Returns 1 on success.
#--
proc kadmin_show { pname } {
    global KADMIN
    global KEY
    global spawn_id

    spawn $KADMIN -m -p krbtest/admin@KRBTEST.COM show $pname
    expect_after {
	timeout {
	    fail "kadmin show $pname"
	    catch "expect_after"
	    return 0
