CFLAGS = $(CCOPTS) $(DEFS) $(LOCALINCLUDES)
RUN_SETUP = @KRB5_RUN_ENV@
PROG_LIBPATH=-L$(TOPLIBD)
PROG_RPATH=$(KRB5_LIBDIR)

OBJS=resolve.o
SRCS=$(srcdir)/resolve.c

all:: resolve

resolve: $(OBJS)
	$(CC_LINK) -o resolve $(OBJS) $(LIBS)

check:: resolve
	$(RUN_SETUP) ./resolve

install::

clean::
	$(RM) resolve

